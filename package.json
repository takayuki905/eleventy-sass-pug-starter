{
  "name": "eleventy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all -p watch:*",
    "watch:scss": "sass --watch src/styles:dist/css",
    "watch:pug": "pug --watch src/pug -o dist/html",
    "watch:11ty": "npx @11ty/eleventy --serve",
    "build": "npm-run-all build:*",
    "build:scss": "sass src/styles:dist/css --style compressed",
    "build:pug": "pug src/pug -o dist/html",
    "build:11ty": "npx @11ty/eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.96.0"
  }
}
